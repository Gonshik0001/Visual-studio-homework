class Button { 
constructor(width, height, type = "button", color = "green"){
this.button_width = width+6;
this.height = height;
this.type = type;
this.color = color;
this.props = {propsWidth: this.button_width, propsHeight: this.height, propsType: this.type, propsColor: this.color
}
onclick() {
    console.log(this.props)
}
}

let ourButton = new Button(30, 2)
ourButton.onClick ();

testButton(ourButton, 36,2, "button", "green")

